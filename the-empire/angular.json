{
  "version": 1,
  "projects": {
    "assets": {
      "projectType": "library",
      "root": "libs/assets",
      "sourceRoot": "libs/assets/src",
      "prefix": "the-empire",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/assets"],
          "options": {
            "jestConfig": "libs/assets/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/assets/src/**/*.ts",
              "libs/assets/src/**/*.html"
            ]
          }
        }
      }
    },
    "death-star": {
      "projectType": "application",
      "root": "apps/death-star",
      "sourceRoot": "apps/death-star/src",
      "prefix": "the-empire",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/death-star",
            "index": "apps/death-star/src/index.html",
            "main": "apps/death-star/src/main.ts",
            "polyfills": "apps/death-star/src/polyfills.ts",
            "tsConfig": "apps/death-star/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/death-star/src/favicon.ico",
              "apps/death-star/src/assets"
            ],
            "styles": ["apps/death-star/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/death-star/src/environments/environment.ts",
                  "with": "apps/death-star/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "death-star:build:production"
            },
            "development": {
              "browserTarget": "death-star:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "death-star:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/death-star/src/**/*.ts",
              "apps/death-star/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/death-star"],
          "options": {
            "jestConfig": "apps/death-star/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "death-star-e2e": {
      "root": "apps/death-star-e2e",
      "sourceRoot": "apps/death-star-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/death-star-e2e/cypress.json",
            "devServerTarget": "death-star:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "death-star:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/death-star-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "star-destroyer": {
      "projectType": "application",
      "root": "apps/star-destroyer",
      "sourceRoot": "apps/star-destroyer/src",
      "prefix": "the-empire",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/star-destroyer",
            "index": "apps/star-destroyer/src/index.html",
            "main": "apps/star-destroyer/src/main.ts",
            "polyfills": "apps/star-destroyer/src/polyfills.ts",
            "tsConfig": "apps/star-destroyer/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/star-destroyer/src/favicon.ico",
              "apps/star-destroyer/src/assets"
            ],
            "styles": ["apps/star-destroyer/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/star-destroyer/src/environments/environment.ts",
                  "with": "apps/star-destroyer/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "star-destroyer:build:production"
            },
            "development": {
              "browserTarget": "star-destroyer:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "star-destroyer:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/star-destroyer/src/**/*.ts",
              "apps/star-destroyer/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/star-destroyer"],
          "options": {
            "jestConfig": "apps/star-destroyer/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "star-destroyer-e2e": {
      "root": "apps/star-destroyer-e2e",
      "sourceRoot": "apps/star-destroyer-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/star-destroyer-e2e/cypress.json",
            "devServerTarget": "star-destroyer:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "star-destroyer:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/star-destroyer-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "the-empire",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "death-star"
}
